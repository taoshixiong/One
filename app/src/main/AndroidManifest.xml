<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taosx.one">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Glide -->
        <meta-data
            android:name="com.taosx.one.Util.Glide.Utils.SimpleGlideModule"
            android:value="GlideModule" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Home.View.HpInfoActivity" />
        <activity
            android:name=".View.Home.View.GalleryImageActivity"
            android:theme="@style/AppTheme.WhiteAtatusBar" />
        <activity android:name=".View.Home.View.EssayListActivity" />
        <activity android:name=".View.Home.View.EssayListInfoActivity" />
        <activity android:name=".View.Home.View.SerialcontentListActivity" />
        <activity android:name=".View.Home.View.SerialcontentListInfoActivity" />
        <activity android:name=".View.Home.View.QuestionListActivity" />
        <activity android:name=".View.Home.View.QuestionInfoActivity"></activity>
    </application>

</manifest>